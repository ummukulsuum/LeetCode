bool isValid(String s) {
  List<String> symbol = [];

  for (var smbl in s.split('')) {
    switch (smbl) {
      case '(':
      case '{':
      case '[':
        symbol.add(smbl); 
        break;

      case ')':
        if (symbol.isEmpty || symbol.removeLast() != '(') return false;
        break;

      case '}':
        if (symbol.isEmpty || symbol.removeLast() != '{') return false;
        break;

      case ']':
        if (symbol.isEmpty || symbol.removeLast() != '[') return false;
        break;
    }
  }

  return symbol.isEmpty;
}

void main() {
  print(isValid("()"));     
  print(isValid("()[]{}")); 
  print(isValid("(]"));    
  print(isValid("([])"));  
  print(isValid("([)]"));   
}
